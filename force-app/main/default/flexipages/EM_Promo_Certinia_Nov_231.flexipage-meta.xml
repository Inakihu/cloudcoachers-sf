                    <value>&lt;p style=&quot;line-height:1.38&quot;&gt;&lt;span style=&quot;font-size:16px;&quot;&gt;&lt;span style=&quot;font-family:Arial,Helvetica,sans-serif;&quot;&gt;&lt;span style=&quot;color:#666b6d;&quot;&gt;&lt;span style=&quot;line-height:1.5;&quot;&gt;Hola,&amp;nbsp;{{{Recipient.FirstName}}}:&lt;br /&gt;
&lt;br /&gt;
