	&lt;li style=&quot;line-height: 1.38;&quot;&gt;&lt;span style=&quot;font-size:16px;&quot;&gt;&lt;span style=&quot;font-family:Arial,Helvetica,sans-serif;&quot;&gt;&lt;span style=&quot;color:#666b6d;&quot;&gt;&lt;span style=&quot;line-height:1.5;&quot;&gt;Optimizaci&amp;oacute;n de tus recursos.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;
	&lt;li style=&quot;line-height: 1.38;&quot;&gt;&lt;span style=&quot;font-size:16px;&quot;&gt;&lt;span style=&quot;font-family:Arial,Helvetica,sans-serif;&quot;&gt;&lt;span style=&quot;color:#666b6d;&quot;&gt;&lt;span style=&quot;line-height:1.5;&quot;&gt;Distribuci&amp;oacute;n del equipo.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;
